<div class="page-header">
  <%= link_to admin_ingressos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    <%= t 'buttons.back' %>
  <% end %>
  <%= link_to edit_admin_ingresso_path(@ingresso), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    <%= t 'buttons.edit' %>
  <% end %>
  <h1><%= t 'buttons.show' %> Ingresso</h1>
</div>
<dl class="dl-horizontal">
  <dt>Preco:</dt>
  <dd><%= @ingresso.preco %></dd>

  <dt>Nome:</dt>
  <dd><%= @ingresso.nome %></dd>

  <dt>Taxa:</dt>
  <dd><%= @ingresso.taxa %></dd>

  <dt>Data:</dt>
  <dd><%= @ingresso.data %></dd>
</dl>
