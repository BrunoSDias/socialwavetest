<div class="page-header">
  <% if notice %>
    <div class="alert alert-success alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <strong><%= notice %></strong>
    </div>
  <% end %>
  <%= link_to admin_ingressos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    <%= t 'buttons.back' %>
  <% end %>
  <%= link_to edit_admin_ingresso_path(@ingresso), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    <%= t 'buttons.edit' %>
  <% end %>
  <h1><%= t 'buttons.show' %> Ingresso</h1>
</div>
<dl class="dl-horizontal">
  <dt>Preco:</dt>
  <dd><%= @ingresso.preco %></dd>

  <dt>Nome:</dt>
  <dd><%= @ingresso.nome %></dd>

  <dt>Taxa:</dt>
  <dd><%= @ingresso.taxa %></dd>

  <dt>Data:</dt>
  <dd><%= l @ingresso.data, format: :short %></dd>
</dl>
